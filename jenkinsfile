// This shows a simple example of how to archive the build output artifacts.
node {
        stage('Checkout repository'){
                git branch: 'development', url: 'https://github.com/manfer993/portafolio.git'
        }
        stage('Install node modules'){
                sh "npm install"
        }
        stage('Build'){
                sh "npm run build"
        }
//        stage('Deploy'){
//                sh "pm2 restart all"
//        }
}